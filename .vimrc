filetype plugin indent on
syntax on
set encoding=utf-8
set clipboard=unnamedplus

let mapleader = "\<Space>"
imap jk <esc>
imap kj <esc>
nmap <leader>wq <esc>:wq<CR>
nmap <leader>q <esc>:q!<CR>
nnoremap <Space> <Nop>
nnoremap <up> <nop>
nnoremap <down> <nop>
nnoremap <left> <nop>
nnoremap <right> <nop>
inoremap <up> <nop>
inoremap <down> <nop>
inoremap <left> <nop>
inoremap <right> <nop>
