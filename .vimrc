filetype plugin indent on
syntax on
set encoding=utf-8
set clipboard=unnamedplus

nnoremap <SPACE> <Nop>
let mapleader = "\<Space>"

nnoremap <up> <nop>
nnoremap <down> <nop>
nnoremap <left> <nop>
nnoremap <right> <nop>
inoremap <up> <nop>
inoremap <down> <nop>
inoremap <left> <nop>
inoremap <right> <nop>
